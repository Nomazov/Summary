{{#with service}}
<div class="card ">
    <div class="card-body text-bg-success text-center">
        <h2 class="mb-3">{{title}}</h2>
        <p class="m-0">{{description}}</p>
        {{#each buttons}}
        <div class="mt-3">
            <a href="{{link}}" target="_blank">
                <button class="btn btn-primary"> {{text}}</button>
            </a>
        </div>
        {{/each}}
    </div>
</div>
{{/with}}