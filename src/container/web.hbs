<div>
    <main>
        <br>
        <hr>
        <section>
            {{> title data="Languages:" big=true}}
            <div>
                {{#with web}}
                <ul>
                    {{#each languages}}
                    <li>
                        <div><em>Name:</em> {{name}}</div>
                        <div><em>Version:</em> {{version}}</div>
                        <div><em>Description:</em> {{description}}</div>
                        <br>

                        {{#if elements}}
                        <div>
                            <ul>
                                <div>Elements:</div>
                                <br>
                                {{#each elements}}
                                <li>
                                    <div>{{name}} - {{description}}</div>
                                    <br>
                                    <ul>
                                        <div>Attributes:</div>
                                        {{#each attributes}}
                                        <li>
                                            <div>{{name}} - {{description}}</div>
                                        </li>
                                        {{/each}}
                                    </ul>
                                </li>
                                <br>
                                {{/each}}
                            </ul>
                        </div>
                        {{/if}}


                        {{#if properties}}
                        <div>
                            <ul>
                                {{#each properties}}
                                <li>
                                    <div>{{name}} - {{description}}</div>
                                    <br>
                                    <ul>
                                        <div>Values:</div>
                                        {{#each values}}
                                        <li>
                                            <div>{{value}} - {{description}}</div>
                                        </li>
                                        {{/each}}
                                    </ul>
                                    <br>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                        {{/if}}


                        {{#if functions}}
                        <div>
                            <ul>
                                <div>Functions:</div>
                                {{#each functions}}
                                <li>
                                    <div>{{name}} - {{description}}</div>
                                    <br>
                                    <ul>
                                        <div>Parameters:</div>
                                        <br>
                                        {{#each parameters}}
                                        <li>
                                            <div>Name: {{name}}</div>
                                            <div>Type: {{type}}</div>
                                            <div>Description: {{description}}</div>
                                        </li>
                                        {{/each}}
                                    </ul>
                                    <br>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                        {{/if}}
                    </li>
                    <br>
                    <br>
                    {{/each}}
                </ul>
                {{/with}}
            </div>
        </section>
        <hr>
        <br>
    </main>
</div>